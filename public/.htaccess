RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP:X-Forwarded-SSL} !on
RewriteCond %{HTTP_HOST} ^covid19\.i315\.my\.id$ [OR]
RewriteCond %{HTTP_HOST} ^www\.covid19\.i315\.my\.id$
RewriteRule ^(.*)$ "https\:\/\/covid19\.i315\.my\.id\/$1" [R=301,L]
Options -Indexes

